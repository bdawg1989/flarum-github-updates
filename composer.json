{
  "name": "vendor/flarum-github-updates",
  "description": "A Flarum extension to post GitHub updates to a specific tag.",
  "type": "flarum-extension",
  "require": {
    "flarum/core": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Vendor\\FlarumGitHubUpdates\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "GitHub Updates",
      "icon": {
        "name": "fab fa-github",
        "backgroundColor": "#000000",
        "color": "#ffffff"
      }
    }
  }
}
